<?xml version="1.0"		encoding="ISO-8859-1"?>
<effects>
	<vertex_shader 
		name="animated_model_render_vs"
		file="./Data/Effects/ForwardShadingUberShader.fx"
		shader_model="vs_4_0"
		entry_point="VS"
		vertex_type="MV_POSITION_WEIGHT_INDICES_NORMAL_TEXTURE_VERTEX"
		preprocessor="HAS_WEIGHT_INDICES;HAS_NORMAL;HAS_UV"/>
	<pixel_shader
		name="animated_model_render_ps"
		file="./Data/Effects/ForwardShadingUberShader.fx"
		shader_model="ps_4_0"
		entry_point="PS"
		preprocessor="HAS_WEIGHT_INDICES;HAS_NORMAL;HAS_UV"/>
	<effect_technique
		name="animated_diffuse_technique"
		vertex_shader="animated_model_render_vs"
		pixel_shader="animated_model_render_ps"/>
	
	
	<vertex_shader
		name="diffuse_vs"
		file="./Data/Effects/ForwardShadingUberShader.fx"		shader_model="vs_4_0"		entry_point="VS"		vertex_type="MV_POSITION_NORMAL_TEXTURE_VERTEX"		preprocessor="HAS_NORMAL;HAS_UV"/>
	<pixel_shader		name="diffuse_ps"		file="./Data/Effects/ForwardShadingUberShader.fx"		shader_model="ps_4_0"		entry_point="PS"		preprocessor="HAS_NORMAL;HAS_UV"/>
	<effect_technique		name="diffuse_technique"		vertex_shader="diffuse_vs"		pixel_shader="diffuse_ps"/>
		
	<vertex_shader
		name="omnilight_vs"
		file="./Data/Effects/OmniLight.fx"
		shader_model="vs_3_0"
		entry_point="VS"
		vertex_type="MV_POSITION_NORMAL_TEXTURE_VERTEX"
		preprocessor=""/>
	<pixel_shader
		name="omnilight_ps"
		file="./Data/Effects/OmniLight.fx"
		shader_model="ps_3_0"
		entry_point="PS"
		preprocessor=""/>
	<effect_technique
		name="omnilight_technique"
		vertex_shader="omnilight_vs"
		pixel_shader="omnilight_ps"/>
	
	
